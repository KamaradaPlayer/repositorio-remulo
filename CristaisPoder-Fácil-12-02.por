programa {
  funcao inicio() {
    
    cadeia elemento

    escreva("Digite para escolher seu elemento: t - terra; g - gelo; f - fogo ")
    leia(elemento)

    escolha (elemento) {
      caso "t":
      escreva("Seu elemento é a Terra")
      pare

      caso "g":
      escreva("Seu elemento é o Gelo")
      pare

      caso "f":
      escreva("Seu elemento é o fogo")
      pare

    }
  }
}
